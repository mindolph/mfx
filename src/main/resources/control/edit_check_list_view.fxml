<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ListView?>
<fx:root type="VBox" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" >
   <children>
      <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox prefWidth="200.0" spacing="4.0" fx:id="hbEditBar">
         <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#onBtnAdd" text="+" />
            <Button fx:id="btnRemove" mnemonicParsing="false" onAction="#onBtnRemove" text="-" />
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </HBox>
   </children>
</fx:root>
