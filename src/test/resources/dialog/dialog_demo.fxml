<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mindolph.mfx.dialog.DialogDemoController">
    <children>
        <VBox layoutX="50.0" layoutY="31.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <Button mnemonicParsing="false" onAction="#onText" text="Single Text" />
                  <Button mnemonicParsing="false" onAction="#onTextBlock" text="Text Block"/>
                  <Button layoutX="93.0" layoutY="12.0" mnemonicParsing="false" onAction="#onTextBlockReadonly" text="Text Block Readonly"/>
                <Button mnemonicParsing="false" onAction="#onChoice" text="Choice" />
                  <Button mnemonicParsing="false" onAction="#onOptions" text="Options " />
              </items>
            </ToolBar>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                <Button mnemonicParsing="false" onAction="#onOpenFile" text="Open File" />
                  <Button mnemonicParsing="false" onAction="#onBrowse" text="Browse" />
                  <Button mnemonicParsing="false" onAction="#onClosing" text="Closing" />
              </items>
            </ToolBar>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <Button mnemonicParsing="false" onAction="#onCustom" text="Custom" />
                  <Button mnemonicParsing="false" onAction="#onCustomDialog" text="Custom Dialog" />
                  <Button mnemonicParsing="false" onAction="#onCustomWithSwingContent" text="Custom With Swing Content" />
                  <Button mnemonicParsing="false" onAction="#onCustomSwingContentDialog" text="Custom Swing Content Dialog" />
              </items>
            </ToolBar>
         </children>
        </VBox>
    </children>
</AnchorPane>
